<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@styles.css" fx:id="gridPane">
    <Button text="Open" GridPane.rowIndex="0" GridPane.columnIndex="0" id="one"
        onAction="#handleClick"/>
    <Button text="Button Two" GridPane.rowIndex="0" GridPane.columnIndex="1" id="two"/>
    <Button text="Button Three" GridPane.rowIndex="0" GridPane.columnIndex="2" id="three"/>
    <Button text="Button Four" GridPane.rowIndex="0" GridPane.columnIndex="3" id="four" fx:id="four"/>
    <Button text="Button Five" GridPane.rowIndex="0" GridPane.columnIndex="4" id="five"/>
    <RadioButton text="Choice One" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <Label text="JavaFX Effects" GridPane.rowIndex="1" GridPane.columnIndex="1" fx:id="label"
        onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit" rotate="90"/>
    <Hyperlink text="www.javafx.com" GridPane.rowIndex="3" GridPane.columnIndex="0"
        onAction="#handleLinkClick"/>
    <WebView fx:id="webView" GridPane.rowIndex="4" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowSpan="3"/>
</GridPane>
<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="sample.Division.DivisionControllerFirstNum"
          hgap="10" vgap="10" alignment="CENTER">
    <Label text="Please enter the first number" alignment="CENTER">
        <font>
            <Font name="System Bold" size="13.0"/>
        </font>
    </Label>
    <TextField fx:id="divisionTextField1" GridPane.rowIndex="1"/>
    <HBox GridPane.rowIndex="2" alignment="CENTER" spacing="5.0">
        <Button fx:id="OKButton" text="OK" prefWidth="80" GridPane.columnIndex="0" onAction="#onOKClick" onKeyPressed="#onOKEnter"/>
        <Button fx:id="cancelButton" text="Cancel" prefWidth="80" GridPane.columnIndex="1" onAction="#onCancelClick" onKeyPressed="#onCancelEnter"/>
    </HBox>
</GridPane>

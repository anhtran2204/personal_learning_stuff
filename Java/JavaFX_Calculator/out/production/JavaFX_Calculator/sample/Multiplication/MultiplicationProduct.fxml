<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="sample.Multiplication.MultiplicationControllerProductScene"
          hgap="10" vgap="10" alignment="CENTER">
    <Label fx:id="label" text="The product of the two numbers are: " alignment="CENTER">
        <font>
            <Font name="System Bold" size="13.0"/>
        </font>
    </Label>
    <HBox GridPane.rowIndex="2" alignment="CENTER" spacing="5.0">
        <Button fx:id="OKButton" text="OK" prefWidth="80" GridPane.columnIndex="0" onAction="#onOKClick" onKeyPressed="#onOKEnter"/>
    </HBox>
</GridPane>
